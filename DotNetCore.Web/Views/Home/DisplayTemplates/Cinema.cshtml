@model DotNetCore.Models.Cinema
<tr>
    <td>@Model.Id</td>
    <td><a asp-controller="Movie" asp-action="Index" asp-route-cinemaId="@Model.Id">@Model.Name</a></td>
    <td>@Model.Location</td>
    <td>@Model.Capacity</td>
    <td>
        <a class="btn btn-sm btn-danger" asp-controller="Home" asp-action="Edit" asp-route-cinemaId="@Model.Id">编辑</a>
    </td>
</tr>